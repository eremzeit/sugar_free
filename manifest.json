{
  "manifest_version": 2,

  "name": "Sugar Free Internet",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["css/styles.css"],
      "js": ["lib/underscore.js", "lib/jquery.js", "build/sugar_free.js"]
    }
  ]
}
